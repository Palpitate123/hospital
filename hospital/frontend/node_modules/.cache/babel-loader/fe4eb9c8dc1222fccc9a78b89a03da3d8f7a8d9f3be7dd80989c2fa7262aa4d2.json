{"ast":null,"code":"export default {\n  name: 'AppointmentManagement',\n  data() {\n    return {\n      appointments: [],\n      departments: [],\n      loading: false,\n      searchForm: {\n        patientName: '',\n        doctorName: '',\n        departmentId: '',\n        status: ''\n      },\n      dateRange: [],\n      currentPage: 1,\n      pageSize: 10,\n      detailDialogVisible: false,\n      selectedAppointment: null,\n      selectedRows: []\n    };\n  },\n  computed: {\n    filteredAppointments() {\n      let filtered = this.appointments.filter(appointment => {\n        // 姓名筛选\n        if (this.searchForm.patientName && !appointment.patientName.toLowerCase().includes(this.searchForm.patientName.toLowerCase())) {\n          return false;\n        }\n        if (this.searchForm.doctorName && !appointment.doctorName.toLowerCase().includes(this.searchForm.doctorName.toLowerCase())) {\n          return false;\n        }\n        // 科室筛选\n        if (this.searchForm.departmentId && appointment.departmentId !== this.searchForm.departmentId) {\n          return false;\n        }\n        // 状态筛选\n        if (this.searchForm.status && appointment.status !== this.searchForm.status) {\n          return false;\n        }\n        // 日期范围筛选\n        if (this.dateRange && this.dateRange.length === 2) {\n          const appointmentDate = new Date(appointment.appointmentDate);\n          const startDate = new Date(this.dateRange[0]);\n          const endDate = new Date(this.dateRange[1]);\n          if (appointmentDate < startDate || appointmentDate > endDate) {\n            return false;\n          }\n        }\n        return true;\n      });\n\n      // 分页\n      const start = (this.currentPage - 1) * this.pageSize;\n      const end = start + this.pageSize;\n      return filtered.slice(start, end);\n    },\n    canBatchConfirm() {\n      return this.selectedRows.every(row => row.status === 'PENDING');\n    }\n  },\n  created() {\n    this.loadData();\n  },\n  methods: {\n    loadData() {\n      this.loading = true;\n      // 模拟数据加载\n      setTimeout(() => {\n        this.appointments = this.getMockAppointments();\n        this.departments = this.getMockDepartments();\n        this.loading = false;\n      }, 500);\n    },\n    handleSearch() {\n      this.currentPage = 1;\n    },\n    handleReset() {\n      this.searchForm = {\n        patientName: '',\n        doctorName: '',\n        departmentId: '',\n        status: ''\n      };\n      this.dateRange = [];\n      this.currentPage = 1;\n    },\n    handleCurrentChange(page) {\n      this.currentPage = page;\n    },\n    handleViewDetail(row) {\n      this.selectedAppointment = row;\n      this.detailDialogVisible = true;\n    },\n    handleConfirm(row) {\n      this.$confirm('确认该预约吗？', '提示', {\n        confirmButtonText: '确定',\n        cancelButtonText: '取消',\n        type: 'warning'\n      }).then(() => {\n        // 模拟确认操作\n        row.status = 'CONFIRMED';\n        row.updateTime = new Date().toISOString();\n        this.$message.success('预约确认成功');\n      }).catch(() => {\n        // 取消操作\n      });\n    },\n    handleCancel(row) {\n      this.$confirm('取消该预约吗？', '提示', {\n        confirmButtonText: '确定',\n        cancelButtonText: '取消',\n        type: 'warning'\n      }).then(() => {\n        // 模拟取消操作\n        row.status = 'CANCELLED';\n        row.updateTime = new Date().toISOString();\n        this.$message.success('预约已取消');\n      }).catch(() => {\n        // 取消操作\n      });\n    },\n    handleBatchCancel() {\n      this.$confirm(`确定要取消选中的 ${this.selectedRows.length} 个预约吗？`, '提示', {\n        confirmButtonText: '确定',\n        cancelButtonText: '取消',\n        type: 'warning'\n      }).then(() => {\n        // 模拟批量取消\n        this.selectedRows.forEach(row => {\n          row.status = 'CANCELLED';\n          row.updateTime = new Date().toISOString();\n        });\n        this.selectedRows = [];\n        this.$message.success('批量取消成功');\n      }).catch(() => {\n        // 取消操作\n      });\n    },\n    handleBatchConfirm() {\n      this.$confirm(`确定要确认选中的 ${this.selectedRows.length} 个预约吗？`, '提示', {\n        confirmButtonText: '确定',\n        cancelButtonText: '取消',\n        type: 'warning'\n      }).then(() => {\n        // 模拟批量确认\n        this.selectedRows.forEach(row => {\n          row.status = 'CONFIRMED';\n          row.updateTime = new Date().toISOString();\n        });\n        this.selectedRows = [];\n        this.$message.success('批量确认成功');\n      }).catch(() => {\n        // 取消操作\n      });\n    },\n    getStatusText(status) {\n      const statusMap = {\n        'PENDING': '待确认',\n        'CONFIRMED': '已确认',\n        'CANCELLED': '已取消',\n        'COMPLETED': '已完成',\n        'NO_SHOW': '已爽约'\n      };\n      return statusMap[status] || status;\n    },\n    getStatusType(status) {\n      const typeMap = {\n        'PENDING': 'warning',\n        'CONFIRMED': 'success',\n        'CANCELLED': 'danger',\n        'COMPLETED': 'info',\n        'NO_SHOW': 'danger'\n      };\n      return typeMap[status] || 'default';\n    },\n    formatDate(date) {\n      if (!date) return '';\n      const d = new Date(date);\n      return `${d.getFullYear()}-${String(d.getMonth() + 1).padStart(2, '0')}-${String(d.getDate()).padStart(2, '0')}`;\n    },\n    formatDateTime(dateTime) {\n      if (!dateTime) return '';\n      const d = new Date(dateTime);\n      return `${this.formatDate(d)} ${String(d.getHours()).padStart(2, '0')}:${String(d.getMinutes()).padStart(2, '0')}:${String(d.getSeconds()).padStart(2, '0')}`;\n    },\n    // 模拟数据\n    getMockDepartments() {\n      return [{\n        id: '1',\n        name: '内科'\n      }, {\n        id: '2',\n        name: '外科'\n      }, {\n        id: '3',\n        name: '儿科'\n      }, {\n        id: '4',\n        name: '妇产科'\n      }, {\n        id: '5',\n        name: '眼科'\n      }, {\n        id: '6',\n        name: '耳鼻喉科'\n      }, {\n        id: '7',\n        name: '口腔科'\n      }, {\n        id: '8',\n        name: '皮肤科'\n      }];\n    },\n    getMockAppointments() {\n      const mockAppointments = [];\n      const departments = this.getMockDepartments();\n      const statuses = ['PENDING', 'CONFIRMED', 'CANCELLED', 'COMPLETED', 'NO_SHOW'];\n      const timeSlots = ['08:00-09:00', '09:00-10:00', '10:00-11:00', '14:00-15:00', '15:00-16:00', '16:00-17:00'];\n      const patientNames = ['张三', '李四', '王五', '赵六', '钱七', '孙八', '周九', '吴十'];\n      const doctorNames = ['李医生', '王医生', '张医生', '刘医生', '陈医生', '杨医生', '黄医生', '周医生'];\n      for (let i = 1; i <= 50; i++) {\n        const randomDate = new Date();\n        randomDate.setDate(randomDate.getDate() + Math.floor(Math.random() * 15) - 5);\n        const department = departments[Math.floor(Math.random() * departments.length)];\n        mockAppointments.push({\n          id: `APT${String(i).padStart(6, '0')}`,\n          patientId: `PAT${String(i).padStart(6, '0')}`,\n          patientName: patientNames[Math.floor(Math.random() * patientNames.length)],\n          patientPhone: `138${String(Math.floor(Math.random() * 100000000)).padStart(8, '0')}`,\n          patientGender: Math.random() > 0.5 ? 'male' : 'female',\n          patientAge: Math.floor(Math.random() * 80) + 18,\n          doctorId: `DOC${String(Math.floor(Math.random() * 20) + 1).padStart(4, '0')}`,\n          doctorName: doctorNames[Math.floor(Math.random() * doctorNames.length)],\n          departmentId: department.id,\n          departmentName: department.name,\n          appointmentDate: randomDate.toISOString().split('T')[0],\n          appointmentTime: timeSlots[Math.floor(Math.random() * timeSlots.length)],\n          status: statuses[Math.floor(Math.random() * statuses.length)],\n          description: Math.random() > 0.5 ? '偶尔头晕，需要检查' : '',\n          createTime: new Date(randomDate.getTime() - Math.random() * 86400000 * 3).toISOString(),\n          updateTime: new Date(randomDate.getTime() - Math.random() * 3600000).toISOString()\n        });\n      }\n\n      // 按预约日期降序排序\n      return mockAppointments.sort((a, b) => new Date(b.appointmentDate) - new Date(a.appointmentDate));\n    }\n  }\n};","map":{"version":3,"names":["name","data","appointments","departments","loading","searchForm","patientName","doctorName","departmentId","status","dateRange","currentPage","pageSize","detailDialogVisible","selectedAppointment","selectedRows","computed","filteredAppointments","filtered","filter","appointment","toLowerCase","includes","length","appointmentDate","Date","startDate","endDate","start","end","slice","canBatchConfirm","every","row","created","loadData","methods","setTimeout","getMockAppointments","getMockDepartments","handleSearch","handleReset","handleCurrentChange","page","handleViewDetail","handleConfirm","$confirm","confirmButtonText","cancelButtonText","type","then","updateTime","toISOString","$message","success","catch","handleCancel","handleBatchCancel","forEach","handleBatchConfirm","getStatusText","statusMap","getStatusType","typeMap","formatDate","date","d","getFullYear","String","getMonth","padStart","getDate","formatDateTime","dateTime","getHours","getMinutes","getSeconds","id","mockAppointments","statuses","timeSlots","patientNames","doctorNames","i","randomDate","setDate","Math","floor","random","department","push","patientId","patientPhone","patientGender","patientAge","doctorId","departmentName","split","appointmentTime","description","createTime","getTime","sort","a","b"],"sources":["src/views/admin/AppointmentManagement.vue"],"sourcesContent":["<template>\n  <div class=\"appointment-management\">\n    <div class=\"page-header\">\n      <h2>预约管理</h2>\n    </div>\n\n    <!-- 搜索筛选区 -->\n    <div class=\"search-filter\">\n      <el-form :inline=\"true\" :model=\"searchForm\" class=\"search-form\">\n        <el-form-item label=\"患者姓名\">\n          <el-input v-model=\"searchForm.patientName\" placeholder=\"请输入患者姓名\"></el-input>\n        </el-form-item>\n        <el-form-item label=\"医生姓名\">\n          <el-input v-model=\"searchForm.doctorName\" placeholder=\"请输入医生姓名\"></el-input>\n        </el-form-item>\n        <el-form-item label=\"科室\">\n          <el-select v-model=\"searchForm.departmentId\" placeholder=\"请选择科室\" clearable>\n            <el-option v-for=\"dept in departments\" :key=\"dept.id\" :label=\"dept.name\" :value=\"dept.id\"></el-option>\n          </el-select>\n        </el-form-item>\n        <el-form-item label=\"预约状态\">\n          <el-select v-model=\"searchForm.status\" placeholder=\"请选择预约状态\" clearable>\n            <el-option label=\"待确认\" value=\"PENDING\"></el-option>\n            <el-option label=\"已确认\" value=\"CONFIRMED\"></el-option>\n            <el-option label=\"已取消\" value=\"CANCELLED\"></el-option>\n            <el-option label=\"已完成\" value=\"COMPLETED\"></el-option>\n            <el-option label=\"已爽约\" value=\"NO_SHOW\"></el-option>\n          </el-select>\n        </el-form-item>\n        <el-form-item label=\"预约日期\">\n          <el-date-picker\n            v-model=\"dateRange\"\n            type=\"daterange\"\n            range-separator=\"至\"\n            start-placeholder=\"开始日期\"\n            end-placeholder=\"结束日期\"\n            style=\"width: 240px\"\n          ></el-date-picker>\n        </el-form-item>\n        <el-form-item>\n          <el-button type=\"primary\" @click=\"handleSearch\">查询</el-button>\n          <el-button @click=\"handleReset\">重置</el-button>\n        </el-form-item>\n      </el-form>\n    </div>\n\n    <!-- 数据表格 -->\n    <div class=\"table-container\">\n      <el-table\n        :data=\"filteredAppointments\"\n        style=\"width: 100%\"\n        stripe\n        v-loading=\"loading\"\n      >\n        <el-table-column type=\"selection\" width=\"55\" align=\"center\"></el-table-column>\n        <el-table-column prop=\"id\" label=\"预约编号\" width=\"120\"></el-table-column>\n        <el-table-column prop=\"patientName\" label=\"患者姓名\" width=\"120\"></el-table-column>\n        <el-table-column prop=\"patientPhone\" label=\"患者电话\" width=\"150\"></el-table-column>\n        <el-table-column prop=\"departmentName\" label=\"科室\" width=\"120\"></el-table-column>\n        <el-table-column prop=\"doctorName\" label=\"医生姓名\" width=\"120\"></el-table-column>\n        <el-table-column prop=\"appointmentDate\" label=\"预约日期\" width=\"120\" :formatter=\"formatDate\"></el-table-column>\n        <el-table-column prop=\"appointmentTime\" label=\"预约时间段\" width=\"150\"></el-table-column>\n        <el-table-column prop=\"status\" label=\"状态\" width=\"100\">\n          <template slot-scope=\"scope\">\n            <el-tag :type=\"getStatusType(scope.row.status)\">{{ getStatusText(scope.row.status) }}</el-tag>\n          </template>\n        </el-table-column>\n        <el-table-column prop=\"createTime\" label=\"创建时间\" width=\"180\" :formatter=\"formatDateTime\"></el-table-column>\n        <el-table-column label=\"操作\" width=\"180\" fixed=\"right\">\n          <template slot-scope=\"scope\">\n            <el-button type=\"primary\" size=\"small\" @click=\"handleViewDetail(scope.row)\">查看详情</el-button>\n            <el-button v-if=\"scope.row.status === 'PENDING'\" type=\"success\" size=\"small\" @click=\"handleConfirm(scope.row)\">确认预约</el-button>\n            <el-button v-if=\"['PENDING', 'CONFIRMED'].includes(scope.row.status)\" type=\"danger\" size=\"small\" @click=\"handleCancel(scope.row)\">取消预约</el-button>\n          </template>\n        </el-table-column>\n      </el-table>\n    </div>\n\n    <!-- 分页 -->\n    <div class=\"pagination\">\n      <el-pagination\n        background\n        layout=\"prev, pager, next, jumper, ->, total\"\n        :total=\"filteredAppointments.length\"\n        :page-size=\"pageSize\"\n        :current-page=\"currentPage\"\n        @current-change=\"handleCurrentChange\"\n      ></el-pagination>\n    </div>\n\n    <!-- 预约详情对话框 -->\n    <el-dialog\n      title=\"预约详情\"\n      :visible.sync=\"detailDialogVisible\"\n      width=\"600px\"\n      top=\"10vh\"\n    >\n      <div v-if=\"selectedAppointment\" class=\"appointment-detail\">\n        <el-descriptions :column=\"1\" border>\n          <el-descriptions-item label=\"预约编号\">{{ selectedAppointment.id }}</el-descriptions-item>\n          <el-descriptions-item label=\"患者信息\">\n            <div>{{ selectedAppointment.patientName }} ({{ selectedAppointment.patientPhone }})</div>\n            <div>{{ selectedAppointment.patientGender === 'male' ? '男' : '女' }}，{{ selectedAppointment.patientAge }}岁</div>\n          </el-descriptions-item>\n          <el-descriptions-item label=\"就诊科室\">{{ selectedAppointment.departmentName }}</el-descriptions-item>\n          <el-descriptions-item label=\"预约医生\">{{ selectedAppointment.doctorName }}</el-descriptions-item>\n          <el-descriptions-item label=\"预约日期\">{{ formatDate(selectedAppointment.appointmentDate) }}</el-descriptions-item>\n          <el-descriptions-item label=\"预约时间\">{{ selectedAppointment.appointmentTime }}</el-descriptions-item>\n          <el-descriptions-item label=\"预约状态\"><el-tag :type=\"getStatusType(selectedAppointment.status)\">{{ getStatusText(selectedAppointment.status) }}</el-tag></el-descriptions-item>\n          <el-descriptions-item label=\"病情描述\">{{ selectedAppointment.description || '无' }}</el-descriptions-item>\n          <el-descriptions-item label=\"创建时间\">{{ formatDateTime(selectedAppointment.createTime) }}</el-descriptions-item>\n          <el-descriptions-item label=\"更新时间\">{{ formatDateTime(selectedAppointment.updateTime) }}</el-descriptions-item>\n        </el-descriptions>\n      </div>\n    </el-dialog>\n\n    <!-- 批量操作工具栏 -->\n    <div class=\"batch-operations\" v-if=\"selectedRows.length > 0\">\n      <el-button type=\"danger\" @click=\"handleBatchCancel\">批量取消</el-button>\n      <el-button type=\"success\" @click=\"handleBatchConfirm\" :disabled=\"!canBatchConfirm\">批量确认</el-button>\n      <span>已选择 {{ selectedRows.length }} 条记录</span>\n    </div>\n  </div>\n</template>\n\n<script>\nexport default {\n  name: 'AppointmentManagement',\n  data() {\n    return {\n      appointments: [],\n      departments: [],\n      loading: false,\n      searchForm: {\n        patientName: '',\n        doctorName: '',\n        departmentId: '',\n        status: ''\n      },\n      dateRange: [],\n      currentPage: 1,\n      pageSize: 10,\n      detailDialogVisible: false,\n      selectedAppointment: null,\n      selectedRows: []\n    }\n  },\n  computed: {\n    filteredAppointments() {\n      let filtered = this.appointments.filter(appointment => {\n        // 姓名筛选\n        if (this.searchForm.patientName && !appointment.patientName.toLowerCase().includes(this.searchForm.patientName.toLowerCase())) {\n          return false\n        }\n        if (this.searchForm.doctorName && !appointment.doctorName.toLowerCase().includes(this.searchForm.doctorName.toLowerCase())) {\n          return false\n        }\n        // 科室筛选\n        if (this.searchForm.departmentId && appointment.departmentId !== this.searchForm.departmentId) {\n          return false\n        }\n        // 状态筛选\n        if (this.searchForm.status && appointment.status !== this.searchForm.status) {\n          return false\n        }\n        // 日期范围筛选\n        if (this.dateRange && this.dateRange.length === 2) {\n          const appointmentDate = new Date(appointment.appointmentDate)\n          const startDate = new Date(this.dateRange[0])\n          const endDate = new Date(this.dateRange[1])\n          if (appointmentDate < startDate || appointmentDate > endDate) {\n            return false\n          }\n        }\n        return true\n      })\n      \n      // 分页\n      const start = (this.currentPage - 1) * this.pageSize\n      const end = start + this.pageSize\n      return filtered.slice(start, end)\n    },\n    canBatchConfirm() {\n      return this.selectedRows.every(row => row.status === 'PENDING')\n    }\n  },\n  created() {\n    this.loadData()\n  },\n  methods: {\n    loadData() {\n      this.loa